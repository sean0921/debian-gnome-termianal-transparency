msgstr "UUID"

#: ../src/gterminal.vala:273 ../src/terminal-options.c:1162
msgid "Set the terminal title"
msgstr "Suidhich tiotal an tèirmineil"

#: ../src/gterminal.vala:273 ../src/terminal-options.c:1163
